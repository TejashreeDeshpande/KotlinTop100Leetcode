fun maxArea(heightArray: IntArray): Int {
        var left = 0
        var right = heightArray.size-1
        var maxArea = 0

        while (left < right) {
            maxArea = Math.max(maxArea, Math.min(heightArray[left], heightArray[right])*(right-left))
            if (heightArray[left] > heightArray[right]) {
                right--
            } else {
                left++
            }
        }
        return maxArea
    }
